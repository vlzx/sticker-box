.email-input {
  box-sizing: border-box;
  height: 90rpx;
  width: 90%;
  padding: 10rpx 20rpx 10rpx 20rpx;
  background-color: #FFFFFF;
  position: relative;
  top: 6%;
  border-radius: 10px;
}

.feedback-placeholder {
  color: rgba(170, 170, 170, 1);
  font-size: 30rpx;
  font-family: Arial;
  margin-top: 20rpx;
}

.email-placeholder {
  color: rgba(170, 170, 170, 1);
  font-size: 30rpx;
  font-family: Arial;
}

button {
  position: relative;
  width: 80%;
  top: 10%;
}